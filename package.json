{
  "name": "next-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:development": "env-cmd -f .env.local next dev",
    "dev:production": "env-cmd -f .env.prod next dev",
    "dev:stage": "env-cmd -f .env.stage next dev",
    "dev": "next dev",
    "turbo": "next dev --turbo",
    "build:development": "env-cmd -f .env.local next build",
    "build:production": "env-cmd -f .env.prod next build",
    "build:stage": "env-cmd -f .env.stage next build",
    "build-stats": "cross-env ANALYZE=true next build",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "preview": "next build && next start",
    "prepare": "husky install"
  },
  "dependencies": {
    "env-cmd": "^10.1.0",
    "next": "13.4.4",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.0.2",
    "@types/node": "^20.3.0",
    "@types/react": "^18.2.11",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "cross-env": "^7.0.3",
    "eslint": "^8.42.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "^13.4.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "prettier": "^2.8.8",
    "typescript": "5.1.3"
  }
}

{
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "endOfLine": "auto",
  "importOrder": [
    "^(react/(.*)$)|^(react$)",
    "^(next/(.*)$)|^(next$)",
    "",
    "^types$",
    "^@/types/(./)$",
    "^@/config/(.*)$",
    "^@/components/(.*)$",
    "^@/constants/(.*)$",
    "^@/context/(.*)$",
    "^@/helper/(.*)$",
    "^@/hoc/(.*)$",
    "^@/hooks/(.*)$",
    "^@/providers/(.*)$",
    "^@/utils/(.*)$",
    "^@/assets/(.*)$",
    "^@/styles/(.*)$",
    "^@/app/(.*)$",
    "",
    "^[./]"
  ],
  "importOrderParserPlugins": ["typescript", "jsx", "decorators-legacy"],
  "jsxSingleQuote": true,
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "printWidth": 100,
  "semi": false,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "all"
}
